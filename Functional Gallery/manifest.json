{
    "manifest_version": 3,
    "name": "Functional Image Gallery",
    "version": "1.0",
    "description": "A Chrome extension for an image gallery with JavaScript.",
    "action": {
      "default_popup": "fun.html",
      "default_icon": {
        "16": "icon.png",
        "48": "icon.png",
        "128": "icon.png"
      }
    },
    "permissions": [
      "storage"
    ]
  }
  