{
    "manifest_version": 3,
    "name": "Strong Password Generator",
    "version": "1.0",
    "description": "Generate strong passwords easily.",
    "permissions": [],
    "action": {
      "default_popup": "index.html"
    },
    "web_accessible_resources": [
